{
  // effects cancel out preconditions
  // action is admissible as long as one set of preconditions is satisfied
  "robot_actions": {
    "default": {
      "preconditions": [[]],
      "effects": []
    },
    "rescue": {
      "preconditions": [[]],
      "effects": ["help", "no_injury"]
    },
    "photo": {
      "preconditions": [[]],
      "effects": ["photo", "no_damage"]
    },
    "measure": {
      "preconditions": [[]],
      "effects": ["measure", "no_pump"] 
    }
  },  
  // action is admissible as long as one set of preconditions is satisfied
  "env_actions": {
      "injury": {
        "preconditions": [["p1", "!no_injury"]]
      },
      "damage": {
        "preconditions": [["p6", "!no_damage"]]
},
      "pump": {
        "preconditions": [["p3", "!no_pump"]]
      }
  },
  // heteregeneous robots
  "robot_group": {
    "1" : ["default", "rescue", "measure"],
    "2" : ["default", "photo", "measure"]
  }
}
  